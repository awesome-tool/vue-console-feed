{
  "name": "vue-console-feed",
  "version": "0.0.5",
  "description": "a repository npm package",
  "scripts": {
    "format": "prettier -w src",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "dev": "vite dev",
    "lint": "eslint src --ext=.ts",
    "lint:fix": "pnpm lint --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.mjs",
  "jsdelivr": "dist/index.umd.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@tachibana-shin/eslint-config": "^1.0.2",
    "@tachibana-shin/eslint-config-vue": "^1.0.5",
    "@types/node": "^17.0.36",
    "@vitejs/plugin-vue": "^3.0.1",
    "bumpp": "^7.1.1",
    "conventional-changelog-cli": "^2.2.2",
    "cspell": "^6.0.0",
    "eslint": "^8.16.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "prettier": "^2.6.2",
    "sass": "^1.54.4",
    "tsup": "^6.0.1",
    "typescript": "^4.7.2",
    "vite": "^3.0.4",
    "vite-plugin-windicss": "^1.8.7",
    "vitest": "^0.13.0",
    "vue-tsc": "^0.39.5",
    "windicss": "^3.5.6"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/tachibana-shin/vue-console-feed.git"
  },
  "homepage": "https://tachibana-shin.github.io/vue-console-feed",
  "keywords": [
    "vue-console-feed",
    "Tachibana Shin",
    "tachibana-shin"
  ],
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pnpm format"
    }
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "linkify": "^0.2.1",
    "linkify-html": "^3.0.5",
    "linkifyjs": "^3.0.5",
    "vue": "^3.2.37"
  }
}
